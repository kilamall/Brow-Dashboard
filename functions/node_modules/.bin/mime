#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/functions/node_modules/mime/node_modules:/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/functions/node_modules:/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/node_modules:/Users/kilam/Desktop/Admin-Github/node_modules:/Users/kilam/Desktop/node_modules:/Users/kilam/node_modules:/Users/node_modules:/node_modules:/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/functions/node_modules/mime/node_modules:/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/functions/node_modules:/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/node_modules:/Users/kilam/Desktop/Admin-Github/node_modules:/Users/kilam/Desktop/node_modules:/Users/kilam/node_modules:/Users/node_modules:/node_modules:/Users/kilam/Desktop/Admin-Github/Brow-Admin-Booking-Dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
