{
  "name": "bueno-brows-monorepo",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev:admin": "pnpm --filter @buenobrows/admin dev",
    "dev:booking": "pnpm --filter @buenobrows/booking dev",
    "dev:legacy": "pnpm --filter @bueno/legacy dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
